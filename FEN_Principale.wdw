   -
     name : AfficherLibelléAvertissement
     procedure_id : 1319225872418948058
     type_code : 14
     code : |1+
      // Résumé : Affiche le libellé d'avertissement
      // Syntaxe :
      //AfficherLibelléAvertissement ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      procedure AfficherLibelléAvertissement()
      
      selon N
      	Cas 1
      		fintimer(1)
      		timer(AfficherLibelléAvertissement,10,1)
      		
      	Cas 5 
      		GRP_AVERTISSEMENT..etat = actif
      		GRP_AVERTISSEMENT..Visible = Vrai
      		fintimer(1)
      		timer(AfficherLibelléAvertissement,Temp2,1)
      		
      	Cas 6
      		GRP_AVERTISSEMENT..Visible = Faux
      		fintimer(1)
      		timer(AfficherLibelléAvertissement,10,1)
      		
      	Autre cas
      fin
      
      N++
      
      Si N = 8 alors 
      	N = 1
      	fintimer(1)
      	timer(AfficherLibelléAvertissement,Temp1,1)	
      fin
      
