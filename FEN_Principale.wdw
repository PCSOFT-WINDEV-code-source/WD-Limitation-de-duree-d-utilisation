   -
     name : ModifierBaseRegistre
     procedure_id : 1319225872419144666
     type_code : 14
     code : |1+
      procedure ModifierBaseRegistre(Clé, Valeur)
      
      RegistreEcrit(CHEMIN_BASE_REGISTRE, Clé, Valeur)
      
      SI ErreurDétectée alors
      	
      	RegistreCréeClé(CHEMIN_BASE_REGISTRE) 	
      	renvoyer RegistreEcrit(CHEMIN_BASE_REGISTRE, Clé, Valeur) 
      	
      sinon
      	
      	renvoyer vrai
      	
      fin
      
