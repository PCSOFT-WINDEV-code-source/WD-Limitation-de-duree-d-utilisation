   -
     name : LireBaseRegistre
     procedure_id : 1319225872419013594
     type_code : 14
     code : |1+
      fonction LireBaseRegistre(sClé)
      
      sRésultatLecture est une chaine
      bOk est un booleen
      
      sRésultatLecture = RegistreLit(CHEMIN_BASE_REGISTRE, sClé, bOk)
      
      si bOk alors
      	renvoyer sRésultatLecture
      sinon
      	Renvoyer ""
      fin
      
     type : 458752
   -
     name : LimitationDansLeTemps
     procedure_id : 1319225872419079130
     type_code : 14
     code : |1+
      // Résumé : Renvoie le temps restant avant la date limite d'utilisation
      // Syntaxe :
      //[ <Résultat> = ] LimitationDansLeTemps ( [<NbJourLimit> [, <ChangeDate>]])
      //
      // Paramètres :
      //	NbJourLimit (valeur par défaut=30) : Nombre de jours restant avant la date de la limitation
      //	ChangeDate (valeur par défaut=0) : Si Vrai alors change la date limite d'utilisation à NbJourLimit
      // Valeur de retour :
      // 	entier : Temps restant avant la date limite d'utilisation
      
      fonction LimitationDansLeTemps(NbJourLimit=30,ChangeDate=Faux)
      
      LimitOk est un booleen = vrai 		//à Faux si et seulement si la date limite est dépassée
      nTempsRestant est un entier   		//Pour calcul du temps restant
      JourDate est un entier   			//Pour calcul du temps restant (date de fin d'utilisation)
      
      sInfo_BR_Lue est une chaine 		//Info lue dans la base de registre pour la durée de la période d'évaluation
      sINFO_BR_àEcrire est une chaine 	//Info à écrire dans la base de registre pour la durée de la période d'évaluation
      
      sVérification est une chaine		//Code de vérification de la durée
      
      //Lecture dans la base de registre
      sInfo_BR_Lue = LireBaseRegistre("ID1")
      
      Si sInfo_BR_Lue[[a 11]] <> "ParameterID" ou ChangeDate alors 
      	
      	//Première utilisation, ou changement de la date limite
      	Si NbJourLimit = -1 alors //Suppression de la limitation 
      		JourDate = NbJourLimit
      	sinon 
      		//MAJ de l'entier correspondant à la date de fin d'utilisation
      		JourDate = DateVersEntier(datesys()) + NbJourLimit
      	fin
      	
      	sINFO_BR_àEcrire = "ParameterID : " + JourDate
      	
      	si ModifierBaseRegistre("ID1",sINFO_BR_àEcrire) = Faux ou NbJourLimit = 0 alors
      		LimitOk = Faux
      	sinon
      		// Code de contrôle de la date
      		ModifierBaseRegistre("ID2",Crypte(NumeriqueVersChaine(JourDate),cCodeVerif,crypteSécurisé,Vrai))
      		nTempsRestant = NbJourLimit
      	fin
      	
      sinon
      	
      	//Lecture du temps restant
      	nTempsRestant = Partieentiere(val(sansespace(sInfo_BR_Lue[[Position(sInfo_BR_Lue,":",1)+1 a]])))
      	
      	//Lecture du code de contrôle de la date
      	sVérification = LireBaseRegistre("ID2")	
      	
      	//Vérification de ce qui a été lu dans la base de registre
      	si Val(DéCrypte(sVérification, cCodeVerif, CrypteSécurisé, Vrai)) <> nTempsRestant alors
      		
      		Erreur(<§$0001§>)	
      		// On bloque l'application
      		RENVOYER 0	
      		
      	FIN
      	
      	Si nTempsRestant <> -1 alors//Limitation en cours
      		
      		nTempsRestant = nTempsRestant - DateVersEntier(datesys())
      		
      		si nTempsRestant <= 0  alors 
      			
      			LimitOk = faux
      			
      			//Plus utilisable à partir de là
      			sINFO_BR_àEcrire = "ParameterID : "+0
      			ModifierBaseRegistre("ID1",sINFO_BR_àEcrire)
      			
      		fin
      		
      	sinon //Limitation supprimée
      		
      		LimitOk = Vrai
      		
      	fin
      	
      fin
      
      si LimitOk alors
      	renvoyer nTempsRestant
      sinon
      	renvoyer 0
      fin
      
