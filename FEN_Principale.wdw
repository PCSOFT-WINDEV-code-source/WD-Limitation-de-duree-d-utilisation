   -
     name : TesterLimitation
     internal_properties : CAAAAAgAAAAaTS46/nxyAK3QVckXzZ2bOauuYYzgQr+bEErwKiZmRkvETpnbh6Fe2+vh410IXHIcSovXsH9IL5A7bioOr8O1spKcMjrfdHd/7yUAEopeyhnTzu5X6/FJgVuP8DtJY8YZ6rEKMOqURj6TO1kKbV9mzKMhY0DTNcH2DhRFwgtPsQwK
     procedure_id : 1319225872419210202
     type_code : 14
     code : |1+
      PROCEDURE TesterLimitation(Periode=30,Maj=Faux)
      
      nTemps est un entier 
      
      //Récupère le temps restant avant la date limite d'utilisation
      nTemps = LimitationDansLeTemps(Periode,Maj)
      
      si nTemps <> -1 alors 
      	
      	si nTemps > 0 alors
      		
      		LIB_Message = gstylo(inoir) + chaineconstruit(<§$0002§>, gstylo(irougeclair) + NumeriqueVersChaine(nTemps) + gstylo(inoir))
      		si nTemps < 15 alors
      			
      			// On avertit l'utilisateur sur le nombre de jours restant
      			info(<§$0003§>,ChaîneConstruit(<§$0004§>, nTemps), <§$0000§>)
      		
      			// Affichage du libellé d'avertissement >> le temps restant est court !! 
      			AfficherLibelléAvertissement()
      			
      		sinon
      			
      			// La limite est encore loin, on ne met pas de message
      			fintimer(1)
      			GRP_AVERTISSEMENT..Visible = Faux			
      			N=1
      			
      		fin
      		
      	sinon
      		
      		// La période d'utilisation est terminée
      		LIB_Message = <§$0005§>
      		
      		// On demande la clé d'activation permettant de débloquer l'application
      		si ouvre(FEN_Demande_CléActivation) = Faux alors
      			
      			//Clé d'activation incorrecte, fin du programme
      			finprogramme(<§$0006§>,<§$0000§>,Faux)
      		
      		sinon
      			
      			// Si la clé d'activation est correcte, la version n'est plus limitée
      			LIB_Message = <§$0007§>
      			
      			TesterLimitation(-1,vrai)
      			
      			// On masque les "rappels de limitation"
      			fintimer(1)
      			GRP_AVERTISSEMENT..Visible=Faux
      			menuinvisible(5)
      			
      		fin
      		
      	fin
      	
      sinon //la limitation a été supprimée, la clé d'activation a été renseignée
      	
      	// On masque les "rappels de limitation", l'application est en version illimitée
      	fintimer(1)
      	
      	LIB_Message = <§$0008§>
      	
      	GRP_AVERTISSEMENT..Visible = Faux
      	menuinvisible(5)
      	
      fin
      
     type : 458752
